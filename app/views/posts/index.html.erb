<div class = "content_area">
  <h1>投稿一覧</h1>

  <ul>
    
    <% @posts.each do |post|%>

    <li class = timestamps><%= post.created_at%></li>
    <li class = content><%= post.content%>
    <%= link_to "詳細を確認する",post_path(post.id)%>
    <%= link_to "編集する",edit_post_path(post.id)%>
    
    </li>
    <%end%>
  </ul>
</div>
